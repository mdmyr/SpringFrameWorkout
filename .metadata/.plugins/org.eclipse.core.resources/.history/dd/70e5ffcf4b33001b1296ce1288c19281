package www.beanScope.com;

public class SedanCar implements VehicleInterface {
   // private counter for the initiations.
	private int counter;
	@Override
	public void breaks() {
		counter= counter+1;
		System.out.println("Sedan :Applied Breaks");

	}

	@Override
	public void gas() {
		
		System.out.println("Sedan : Push the gas!!!");
	}

}
